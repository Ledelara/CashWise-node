generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

model User {
  id             String @id @default(auto()) @map("_id") @db.ObjectId
  nome           String
  email          String @unique
  senhaLogin     String
  senhaTransacao String
  saldo          Float  @default(0.0)
  numeroConta    String @unique
}
